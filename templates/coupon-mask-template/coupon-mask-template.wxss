.couponMask{
  position: fixed;
  top:0;
  left:0;
  height:100%;
  width:100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 11000;
}
.coupon-container{
  position: fixed;
  z-index: 11000;
  width:550rpx;
  top:50%;
  left:50%;
  transform: translateX(-50%) translateY(-50%);
  background: url('https://static.vitasantee.com/2261B35B-4527-466E-9A6D-4C4EFF4F34A1@2x.png') no-repeat;
  background-size: 100% 100%;
  padding:40rpx 40rpx 0;
  border-radius: 3px;
}
.coupon-title{
  color:#fff;
  font-size: 22px;
  line-height: 50px;
  text-align: center;
  width:100%;
}
.coupon-list{
  background: #fff;
  border-radius: 3px;
  padding:30rpx;
  width:100%;
}
.coupon-item{
  margin-bottom: 30rpx;
  height: 120rpx;
  width:100%;
  background: url('https://static.vitasantee.com/12189685-6F3B-4689-A8A3-253FF0620C59@2x.png') no-repeat;
  background-size: 100% 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.coupon-item:last-child{
  margin-bottom: 0;
}
.coupon-item-left{
  width:170rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.coupon-item-right{
  padding-left: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.couponBtn{
  background: #7fabc0;
  color:#fff;
  font-size: 16px;
  height: 76rpx;
  border-radius: 38rpx;
  width:280rpx;
  margin:40rpx auto;
  text-align: center;
  line-height: 76rpx;
}
