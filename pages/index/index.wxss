  overflow: hidden;
}

/* 限时推荐 */
.Timelimit{
  border-top: 1px solid #eaeaec;
}
.Timelimit_item{
  display: flex;
  justify-content: flex-start;
  flex-wrap: nowrap;
  align-items: center;
  padding-bottom: 20rpx;
}
.Timelimit_item_detail{
  position: relative;
  padding:10rpx;
  width:160rpx;
  margin-right:17rpx;
  box-shadow: 0px 0px 20px rgba(0,0,0,0.1);
}
.Timelimit_item_detail:last-child{
  margin-right: 0;
}
.timeLimit-box{
  margin-left: 10rpx;
}
.timeLimit-time{
  background: #e40046;
  border-radius: 3px;
  height:40rpx;
  line-height: 40rpx;
  width:40rpx;
  text-align: center;
  color:#fff;
  font-size: 12px;
  margin:0 6rpx;
  padding:0 4rpx;
}
.Timelimit_item_detail>image{
  width:140rpx;
  margin-bottom: 10rpx;
}
.Timelimit_no{
  position: absolute;
  left:10rpx;
  top:10rpx;
  border-radius: 3px;
  line-height: 140rpx;
  text-align: center;
  width:140rpx;
  height:140rpx;
  color:#fff;
  background: rgba(0,0,0,0.3);
}
.Timelimit_progress{
  width:100%;
  height:6px;
  border-radius: 5px;
  border:1px solid #e40046;
}
.Timelimit_progress_active{
  width:50%;
  height: 4px;
  background: #e40046;
}
.Timelimit_progress_no{
  border: 1px solid #93949a;
}
.Timelimit_progress_no>.Timelimit_progress_active{
  width:100%;
  background: #93949a;
}
.Timelimit_text{
  font-size: 12px;
  margin: 10rpx 0;
  display: -webkit-box; 
  -webkit-line-clamp: 2; 
  -webkit-box-orient: vertical; 
  overflow: hidden;
}
.Timelimit_price{
  color:#e40046;
  font-size: 14px;
}
.Timelimit_price>text{
  font-size: 12px;
  color:#999;
}

