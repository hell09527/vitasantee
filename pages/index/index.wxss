  margin:20rpx;
}
.recommended_item_box{
  display: flex;
  justify-content: space-around;
}
.recommended_item{
  padding:20rpx 10rpx;
  width:50%;
}
.recommended_item:first-child{
  border-right: 1px solid #eaeaec;
}
