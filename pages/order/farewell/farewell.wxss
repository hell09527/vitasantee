.mask-layer-control{
  overflow: hidden;
  position: fixed;
  z-index: 1000;
  left: 0px;
  right: 0px;
  bottom: 0px;
  visibility: visible;
  opacity: 1;
  background: white;
}

.mask-layer-control .header{
  padding: 10px 0;
  position: relative;
  line-height: 30px;
  font-size: 16px;
  text-align: center;
  border-bottom: 1px solid #e5e5e5;
  color: #333;
}

.mask-layer-control .close{
  position: absolute;
  right: 6px;
  top: 5px;
  padding: 10px;
  height: 22px;
  width: 22px;
}

.mask-layer-control .list{
  margin: 0;
  padding: 0;
  padding-left: 10px;
  list-style: none;
  font-size: 14px;
  box-sizing: border-box;
}

.mask-layer-control .list .ul{
  max-height: 240px;
  overflow-y: scroll;
  background-color: #fff;
}

.mask-layer-control .list .item{
  cursor: pointer;
  display: block;
  position: relative;
  line-height: 1.5;
  border: 0;
  padding: 5px 20px 5px 30px;
  border-bottom: 1px solid #e5e5e5;
}

.mask-layer-control .check-img{
  position: absolute;
  left: 2px;
  top: 11px;
  width: 22px;
  height: 22px;
}

.mask-layer-control .list .item .single{
  line-height: 30px;
  height: inherit;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}
.singles{
  line-height: 45px;
  height: inherit;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  color:red;
  -webkit-line-clamp: 3;
  overflow: hidden;
}
.mask-layer-control .footer{
  position: relative;
  padding: 20px 10px;
  padding-left: 0;
  margin: 0;
  margin-left: 10px;
  min-height:unset;
}

.btn-green{
  background-color: #06bf04;
  border-color: #03b401;
  border: 0;
  text-align: center;
