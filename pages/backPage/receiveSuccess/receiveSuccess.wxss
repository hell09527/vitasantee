scroll-view{
  height:100%;
  background: #f1f1f1;
}
.top{
  position: absolute;
  top:10%;
  left:50%;
  transform: translateX(-50%);
  text-align: center;
}
.top>image{
  width:120px;
  display: block;
  margin:0 auto 40rpx;
}
.container{
  position: absolute;
  top:35%;
  display: flex;
  justify-content: space-around;
  flex-direction: row;
  padding:0 10px;
  height:30%;
  align-items: center;
  width:100%;
}
