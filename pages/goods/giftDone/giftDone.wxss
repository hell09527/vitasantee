.show-btn {
  margin-top: 100 rpx;
  color: #22cc22;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.7;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 80%;
  overflow: hidden;
  position: fixed;
  top:30%;
  left: 0;
  right: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: auto;
  border-radius: 0 rpx;
}

.modal-title {
  padding: 25rpx 0;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
}

.modal-title-desc{
  font-size:24rpx;
  color:gray;
}

.modal-content {
  padding: 50rpx 32rpx;
  border-top:1rpx solid #DDDDDD;
  border-bottom:1rpx solid #DDDDDD;
}

.modal-content .flex-wrp .flex-item{
  margin-bottom:10rpx;
}

.modal-input {
  display: flex;
  background: #fff;
  border: 2rpx solid #ddd;
  border-radius: 4rpx;
  font-size: 28rpx;
}


.input {
  width: 100%;
  height: 82rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  color: #333;
}

input-holder {
  color: #666;
  font-size: 28rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}

.btn-cancel {
  width: 50%;
  color: #878787;
  text-align: center;
  border-right: 1px solid #dedede;
}

.btn-confirm {
  width: 50%;
  color: #000;
  font-weight:900;
  text-align: center;
}

.goods-type-icon{
  background:#000;
  color:#fff;
  font-size:12px;
  padding:1px 4px;
  margin-right:3px;
  float: left;
  height: 20px;
  text-align: center;
}

/* cust end ========================= */

input {
  color: initial;
}

.store-title{
  line-height: 40px;
  display: flex;
  align-items: center;
  padding:0 20rpx;
  width:100%;
  justify-content: space-between;
}

.cart-detail {
  margin-top: 2px;
}

.cart-prolist {
  padding:0 10px;
  background-color: #fff;
  margin-bottom: 80px;
}

.cart-prolist-ul {
  padding: 0px;
  background: #fff;
  border: 0;
}

.cart-prolist .title {
  font-size: 14px;
  color: #333;
  padding: 10px 0 10px 0;
  font-weight: normal;
}

.custom-store-img {
  display: inline-block;
  vertical-align: middle;
  height: 18px;
  width: 18px;
  background-size: 18px 18px;
  margin-right: 10px;
}

.cart_ed {
  float: right;
}

.cart_ed .a {
  cursor: pointer;
  display: inline;
}

.cart-list-li {
  margin: 0px;
  border: 0;
  border-bottom: 1px solid #F8F8F8;
  justify-content: space-between;
  width:100%;
  padding: 10px 0;
}

.checkbox {
  cursor: pointer;
  width: 20px;
  height: 20px;
  margin-right: 10rpx;
}

.cart-list-li .product {
  flex: 1;
}

.cart-list-li .product .pic {
  margin-right: 10px;
  border: 1px solid #E4E3DD;
  border-radius: 3px;
}

.cart-list-li .pic a {
  width: 100%;
  text-align: center;
}

.cart-list-li .product .pic image {
  width: 80px;
  height: 80px;
  vertical-align: middle;
}

.cart-list-li .product .info {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  /* height: 80px; */
  flex: 1;
}

.cart-list-li .product .info-name {
  font-size: 12px;
}


.a{
  display: table;
}

 .name {
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  font-size: 14px;
  line-height: 22px;
}

.cart-list-li .product .info-name .a .sku {
  display: block;
  height: 17.5px;
  overflow: hidden;
  text-overflow: ellipsis;
}

.cart-list-li .product .info-name .span {
  color: #999999;
}

.cart-list-li .number {
  color: red;
  margin: 5rpx 0;
  text-align: left;
  line-height: 1.5;
}

.edit_num{
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.cart-list-li .ui-number .decrease, .cart-list-li .ui-number .increase {
  color: initial;
  width: 25px;
  height:1.4rem;
  line-height: 1.4rem;
  text-align: center;
  background: #fbfbfb;
}
.ui-number{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  border:1px solid #ccc;
}
.cart-list-li .ui-number .decrease {
  color: initial;
  display: inline-block;
  border-top-left-radius: 2px;
  border-bottom-left-radius: 2px;
  font-size: 20px;
}

.num {
  border-radius: 0;
  -webkit-border-radius: 0;
  border-left: 1px;
  -webkit-border-left: 1px;
  box-shadow: none;
  margin-left: -1px;
  margin-right: -1px;
}

.cart-list-li .ui-number .increase {
  display: inline-block;
  border-top-right-radius: 2px;
  border-bottom-right-radius: 2px;
  font-size: 20px;
}

.cart-list-li .ui-number .num {
  padding: 0 5px;
  height:1.4rem;
  line-height: 1.4rem;
  width: 30px;
  text-align: center;
}

.fixed {
  width: 100%;
  left: 0;
  position: fixed;
  top: 0;
  z-index: 99;
}

.fixed.bottom {
  bottom: -1px;
  top: auto;
}

body .fixed.bottom {
  bottom: 0;
}

.btn_wrap_static {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 50px;
  background: #fff;
  border-top: 1px solid #fff;
  z-index: 5;
  padding: 0;
  box-shadow: 1px 0 10px 1px #DDD;
}

.btn {
  width: 100%;
  overflow: hidden;
  margin: 0 0 20px 0;
}

.btn_wrap .btn, .s-buy .btn {
  float: left;
  height: 50px;
  line-height: 50px;
  text-decoration: none;
  display: block;
  text-align: center;
  font-size: 16px;
  color: #FFF;
  width: 21%;
  cursor: pointer;
  background: #fff;
}

#div_selected .div {
  float: left;
  height: 38px;
  width: 20px;
  margin-top: 5px;
  padding-left: 10px;
}

.div image {
  width: 20px;
  height: 20px;
}

#div_selected .span {
  font-size: 14px;
  color: #333;
  position: absolute;
  left: 32px;
}

#price_info {
  float: left;
  width: 39%;
  background: #fff;
  height: 50px;
  line-height: 50px;
  /* color: #f85;  */
  color: #000000;
  font-size: 14px;
  text-align: right;
  padding-bottom: 4px;
  position: relative;
  right: 5px;
}

#price_info .price {
  display: inline;
}

.btn_wrap .btn_buy {
  width: 40%;
  display: block;
  border: 0;
  margin: 0;
  cursor: pointer;
  height: 50px;
  line-height: 50px;
  margin-top: -1px;
  box-shadow: 1px 0 10px 1px #DDD;
}

.btn_wrap .btn_buy.delete {
  background: rgb(204, 204, 204);
  border-top: 1px solid rgb(204, 204, 204);
}

