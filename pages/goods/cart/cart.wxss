.cart-list-li {
  border-bottom: 1px solid #F8F8F8;
  justify-content: space-between;
  width:100%;
  padding: 20rpx;
  position: relative;
  z-index: 10;
  min-height:240rpx;
  box-sizing: border-box;
  height: 100%;
  overflow-y: hidden;
  overflow:auto;-webkit-overflow-scrolling: touch;
}

.checkbox {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
 -webkit-transition: all 0.4s;
 transition: all 0.4s;
 -webkit-transform: translateX(90px);
 transform: translateX(90px);
 margin-left: -90px;
}
.checkbox>image{
  width: 40rpx;
  height: 40rpx;
  display: inline-block
}

.cart-list-li .product {
  width:670rpx;
  padding:0 10px 0 0;
    -webkit-transition: all 0.4s;
 transition: all 0.4s;
 -webkit-transform: translateX(90px);
 transform: translateX(90px);
 /* margin-left: -90px  */
 position: relative;
 z-index: 10;
  -webkit-backface-visibility: hidden;
}

.cart-list-li .product .pic {
  margin-right: 10px;
  border: 1px solid #E4E3DD;
  border-radius: 3px;

}

.cart-list-li .pic a {
  width: 100%;
  text-align: center;
}

.cart-list-li .product .pic image {
  width: 80px;
  height: 80px;
  vertical-align: middle;
}

.cart-list-li .product .info {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  /* height: 80px; */
  flex: 1;
  position: relative;
  z-index: 10;
}

.cart-list-li .product .info-name {
  font-size: 12px;
}
.a{
  display: table;
}
 .name {
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  font-size: 14px;
}

.cart-list-li .product .info-name .a .sku {
  display: block;
  height: 17.5px;
  overflow: hidden;
  text-overflow: ellipsis;
}

.cart-list-li .product .info-name .span {
