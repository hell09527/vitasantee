.cf-container{
  top: 0;
  width: 100%;
  z-index: 6;
  background: #fff;
  overflow: hidden;
  font-size: 14px;
  border-bottom: 1px solid #E2E2E2;
  max-width: 640px;
  margin: auto;
}

.cf-container .cf-content{
  color: #333;
  text-align: center;
  display: flex;
  justify-content: space-around;
  height:40px;
  align-items: center;
}

.cf-container .cf-tab-item{
  border-bottom: 2px solid transparent;
  color: #999;
  font-size: 14px;
  line-height: 26px;
  height: 26px;
}


.cf-container .cf-tab-item.select{
  border-bottom: 2px solid #e40046;
  color: #e40046;
}

.collection_goods_list{
  width: 100%;
  padding:0 10px 10px;
  background: #fff;
}

.collection_goods_list .goods_info{
  width: 100%;
  overflow: hidden;
  border-bottom: 1px solid #F3F3F4;
}

.collection_goods_list .last-child{
  border-bottom: none;
}

.collection_goods_list .goods_info .collection_time{
  width: 100%;
  height: 30px;
  line-height: 30px;
  padding-left: 10rpx;
  color: #333;
}

.collection_goods_list .goods_info .goods_img{
  display: block;
  height: 100px;
  width: 100px;
  text-align: center;
  line-height: 100px;
  margin-right: 10px;
  margin-bottom: 5px;
}

.collection_goods_list .goods_info .goods_img image{
  width: 100%;
  height: 100%;
}

.collection_goods_list .goods_info .goods_img .pic{
  max-height: 100%;
  max-width: 100%;
  height: auto;
  vertical-align: middle;
}

.collection_goods_list .goods_info .data_info{
  display: block;
  height: 100px;
  flex: 1;
  position: relative;
}

.collection_goods_list .goods_info .data_info .goods_name{
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  line-height: 22px;
  display: -webkit-box;
}

.collection_goods_list .goods_info .data_info .price_share{
  position: absolute;
  height: 30px;
  line-height: 30px;
  bottom: 0;
}

.collection_goods_list .goods_info .data_info .price_share .price{
  display:  block;
  float: left;
  /* color: red; */
}

.collection_goods_list .goods_info .data_info .price_share .cancel_collection{
  display: block;
  float: right;
}

.price_share{
  width: 100%;
}

.collection_goods_empty{
  line-height: 60px;
  text-align: center;
  color: #666;
}
