.hide{
  display: none !important;
}

.cf-content{
  display: flex;
  justify-content: space-around;
  height: 40px;
  align-items: center;
  padding:0 20rpx;
  background: #fff;
}

.cf-tab-item{
  color: #999;
  font-size: 14px;
  border-bottom: 2px solid transparent;
  height:26px;
  line-height: 26px;
}

.select{
  border-bottom: 2px solid #e40046;
  color: #e40046;
}

.null_default{
  padding-bottom: 40px;
}

.null_default .icon-nocontent-coupon{
  display: block;
  margin-top: 10px;
  width: 100%;
  height: 120px;
  line-height: 120px;
  text-align: center;
}

.icon-nocontent-coupon image{
  width: 90px;
  vertical-align: middle;
  margin-bottom: 40rpx;
}

.null_default .nulltext{
  display: block;
  width: 100%;
  text-align: center;
  font-size: 14px;
  color: #999;
}

.coupon-contianer {
  margin-top: 8px;
  padding: 20rpx;
}

.coupon-contianer .coupon-item {
  margin-bottom: 20rpx;
  height: 100px;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.coupon-item-user{
  /* background: url('https://static.vitasantee.com/coupon@2x.png') no-repeat; */
  background-size: 100% 100%;
}
.coupon-item-nouser{
  /* background: url('https://static.vitasantee.com/coupon@2x.png') no-repeat; */
  background-size: 100% 100%;
}
.coupon-left{
  background: url('https://static.vitasantee.com/coupon-1@2x.png') no-repeat;
  background: #e40046;
  width:210rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding:20rpx;
  height: 100px;
}
.backgroundColor{
  background: #d6d7d9;
}
.coupon_right_box{
  background: url('https://static.vitasantee.com/coupon-2@2x.png') no-repeat;
  background: #fff;
  flex: 1;
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding:20rpx;
  height: 100px;
}
.coupon-item-user .coupon-left-price{
  color:#fff;
}
.coupon-item-nouser .coupon-left-price{
  color:#999;
}
.coupon-middle{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
}
.coupon-right>view{
  background: #005882;
  color:#fff;
  height:26px;
  line-height: 26px;
  border-radius: 13px;
  font-size: 12px;
  width:120rpx;
  text-align: center;
}